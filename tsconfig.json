{

    "compilerOptions": {
        "allowJs": true,
        "target": "ES5",
        "outDir": "./dist",
        "moduleResolution": "Node",
        "lib": ["ES2015","DOM","DOM.Iterable"],
        "noImplicitAny": true,
        "typeRoots": ["./node_modules/@types","./types"],
        
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules","dist"],
}

// "typeRoots": ["./node_modules/@types","./types"] 외부라이브러리 타입폴더